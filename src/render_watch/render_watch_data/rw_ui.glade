            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="hscrollbar-policy">never</property>
            <child>
              <object class="GtkViewport" id="completed_page_viewport">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="shadow-type">none</property>
                <child>
                  <object class="GtkListBox" id="completed_list">
                    <property name="visible">True</property>
                    <property name="can-focus">False</property>
                    <property name="selection-mode">none</property>
                    <signal name="add" handler="on_completed_list_add" swapped="no"/>
                    <signal name="remove" handler="on_completed_list_remove" swapped="no"/>
                    <child type="placeholder">
                      <object class="GtkBox" id="completed_placeholder_box">
                        <property name="visible">True</property>
                        <property name="can-focus">False</property>
                        <property name="orientation">vertical</property>
                        <property name="spacing">20</property>
                        <child type="center">
                          <object class="GtkImage" id="completed_placeholder_icon">
                            <property name="visible">True</property>
                            <property name="can-focus">False</property>
                            <property name="opacity">0.5</property>
                            <property name="pixel-size">128</property>
                            <property name="icon-name">action-unavailable-symbolic</property>
                            <property name="icon_size">6</property>
                          </object>
                          <packing>
                            <property name="expand">False</property>
                            <property name="fill">True</property>
                            <property name="position">1</property>
                          </packing>
                        </child>
                        <child>
                          <object class="GtkLabel" id="completed_placeholder_label">
                            <property name="visible">True</property>
                            <property name="sensitive">False</property>
                            <property name="can-focus">False</property>
                            <property name="valign">start</property>
                            <property name="label" translatable="yes">No Completed Tasks</property>
                          </object>
                          <packing>
                            <property name="expand">True</property>
                            <property name="fill">True</property>
                            <property name="pack-type">end</property>
                            <property name="position">2</property>
                          </packing>
                        </child>
                      </object>
                    </child>
                    <style>
                      <class name="flat"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="name">Completed Page</property>
            <property name="title" translatable="yes">Completed</property>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="header_bar">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <property name="spacing">5</property>
        <property name="show-close-button">True</property>
        <child type="title">
          <object class="GtkStackSwitcher" id="page_switcher">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="halign">center</property>
            <property name="homogeneous">True</property>
            <property name="icon-size">0</property>
            <property name="stack">page_stack</property>
          </object>
        </child>
        <child>
          <object class="GtkMenuButton" id="application_options_menubutton">
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="focus-on-click">False</property>
            <property name="receives-default">True</property>
            <property name="tooltip-text" translatable="yes">Show settings</property>
            <property name="margin-end">5</property>
            <property name="draw-indicator">True</property>
            <property name="popover">application_options_popover</property>
            <child>
              <object class="GtkImage" id="application_options_icon">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="icon-name">open-menu-symbolic</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="pack-type">end</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="toggle_settings_sidebar_button">
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="receives-default">True</property>
            <property name="always-show-image">True</property>
            <signal name="clicked" handler="on_toggle_settings_sidebar_button_clicked" swapped="no"/>
            <child>
              <object class="GtkImage" id="toggle_settings_sidebar_icon">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="icon-name">sidebar-show-right-symbolic</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="pack-type">end</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkFileChooserButton" id="output_directory_chooserbutton">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="tooltip-text" translatable="yes">Choose the output location for all inputs</property>
            <property name="halign">end</property>
            <property name="action">select-folder</property>
            <property name="title" translatable="yes">Output Location</property>
            <signal name="file-set" handler="on_output_directory_chooserbutton_file_set" swapped="no"/>
          </object>
          <packing>
            <property name="pack-type">end</property>
            <property name="position">2</property>
          </packing>
        </child>
        <child>
          <object class="GtkBox" id="add_inputs_box">
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <child>
              <object class="GtkButton" id="add_inputs_button">
                <property name="label" translatable="yes">Add</property>
                <property name="visible">True</property>
                <property name="can-focus">True</property>
                <property name="receives-default">True</property>
                <signal name="clicked" handler="on_add_inputs_button_clicked" swapped="no"/>
                <style>
                  <class name="suggested-action"/>
                </style>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkComboBoxText" id="add_inputs_type_combobox">
                <property name="visible">True</property>
                <property name="can-focus">False</property>
                <property name="tooltip-text" translatable="yes">Set the input type</property>
                <property name="active">0</property>
                <property name="popup-fixed-width">False</property>
                <items>
                  <item id="&lt;Enter ID&gt;" translatable="yes">Files</item>
                  <item translatable="yes">Folders</item>
                </items>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
            <style>
              <class name="linked"/>
            </style>
          </object>
          <packing>
            <property name="position">3</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
